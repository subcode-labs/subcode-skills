{
	"name": "subcode-skills",
	"version": "0.1.3",
	"description": "Portable Claude skills for enhanced developer experience",
	"type": "module",
	"bin": {
		"subcode-skills": "./packages/subcode/dist/index.mjs",
		"subcode": "./packages/subcode/dist/index.mjs"
	},
	"files": [
		"packages/subcode/dist",
		"skills",
		"schemas",
		"lib"
	],
	"scripts": {
		"build": "cd packages/subcode && npm run build",
		"prepublishOnly": "npm run build",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "^1.1.14",
		"typescript": "^5.7.2"
	},
	"workspaces": ["skills/*", "packages/*"],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/subcode-labs/subcode-skills.git"
	},
	"bugs": {
		"url": "https://github.com/subcode-labs/subcode-skills/issues"
	},
	"homepage": "https://github.com/subcode-labs/subcode-skills#readme",
	"keywords": ["claude", "skills", "developer-experience", "worktrees", "git"]
}
